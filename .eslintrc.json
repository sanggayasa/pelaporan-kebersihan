{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "quotes": "off",
    "no-underscore-dangle": "off",
    "no-console": 0,
    "no-unused-vars": "off",
    "array-callback-return": "off",
    "indent": "off",
    "comma-dangle": "off"
  }
}
